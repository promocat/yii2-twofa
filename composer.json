{
  "name": "promocat/yii2-twofa",
  "type": "yii2-extension",
  "description": "Provides TOTP and QR codes for use with an authenticator like the one from Google or Authy",
  "keywords": [
    "yii2",
    "otp",
    "totp",
    "2fa",
    "Two Factor Authentication",
    "google authenticator",
    "authy",
    "qr"
  ],
  "homepage": "https://github.com/promocat/yii2-twofa",
  "license": "MIT",
  "authors": [
    {
      "name": "Brandon Tilstra",
      "email": "brandontilstra@promocat.nl",
      "role": "Developer"
    }
  ],
  "require": {
    "yiisoft/yii2": "~2.0.0",
    "bacon/bacon-qr-code": "~1.0",
    "pragmarx/google2fa": "^3.0"
  },
  "autoload": {
    "psr-4": {
      "promocat\\twofa\\": "src/"
    }
  }
}
